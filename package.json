{
  "name": "rtxd",
  "version": "0.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Next Generation Serial Debug Tools",
  "author": "Evanxiao <1993996310@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
